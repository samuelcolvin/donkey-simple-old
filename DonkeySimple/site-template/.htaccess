Options +ExecCGI +FollowSymLinks
AddHandler cgi-script .cgi

RewriteEngine On

RewriteCond %{REQUEST_URI} !^.*/index.cgi$
RewriteCond %{REQUEST_URI} !^.*/edit/repos/.*/static/.*$
RewriteCond %{REQUEST_URI} !^.*/edit/static/.*$
RewriteCond %{REQUEST_URI} !^.*/edit/repos.zip$
RewriteCond %{REQUEST_URI} !^.*/edit/site.zip$
RewriteRule ^(.*) index.cgi
